{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@utils/*": ["src/utils/*"],
            "@content/*": ["src/content/*"],
            "@data/*": ["src/data/*"]
        }
    }
}
